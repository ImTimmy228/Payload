local a=workspace:FindFirstChildOfClass('Terrain')a.WaterWaveSize=0;a.WaterWaveSpeed=0;a.WaterReflectance=0;a.WaterTransparency=0;Lighting.FogEnd=9e9;settings().Rendering.QualityLevel=1;for b,c in pairs(game:GetDescendants())do if c:IsA("Part")or c:IsA("UnionOperation")or c:IsA("MeshPart")or c:IsA("CornerWedgePart")or c:IsA("TrussPart")then c.Material="Plastic"c.Reflectance=0 elseif c:IsA("Decal")then c.Transparency=1 elseif c:IsA("ParticleEmitter")or c:IsA("Trail")then c.Lifetime=NumberRange.new(0)elseif c:IsA("Explosion")then c.BlastPressure=1;c.BlastRadius=1 end end;for b,c in pairs(Lighting:GetDescendants())do if c:IsA("BlurEffect")or c:IsA("SunRaysEffect")or c:IsA("ColorCorrectionEffect")or c:IsA("BloomEffect")or c:IsA("DepthOfFieldEffect")then c.Enabled=false end end;workspace.DescendantAdded:Connect(function(d)coroutine.wrap(function()if d:IsA('ForceField')then RunService.Heartbeat:Wait()d:Destroy()elseif d:IsA('Sparkles')then RunService.Heartbeat:Wait()d:Destroy()elseif d:IsA('Smoke')or d:IsA('Fire')then RunService.Heartbeat:Wait()d:Destroy()end end)()end)

